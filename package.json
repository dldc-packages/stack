{
  "name": "staack",
  "version": "1.0.0",
  "description": "A library to create type-safe opaque stacks",
  "keywords": [
    "stack",
    "typescript"
  ],
  "homepage": "https://github.com/etienne-dldc/staack#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/staack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/staack.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "pnpm run build && pnpm run test && bumpp && git push --follow-tags",
    "test": "pnpm run lint && vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vitest/coverage-v8": "^0.32.0",
    "vitest": "^0.32.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "bumpp": "^9.1.1",
    "denoify": "^1.5.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@8.6.1",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
